// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Leads {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  createdAt DateTime @default(now())
}

model FreeArticle {
  id        Int      @id @default(autoincrement())
  name      String?
  email     String
  subject   String?
  message   String?
  createdAt DateTime @default(now())
}

model ContactMessage {
  id        Int      @id @default(autoincrement())
  name      String?
  email     String
  subject   String?
  message   String?
  createdAt DateTime @default(now())
}
